    "addstring_identifiers": {
      "patterns": [
        {
          "name": "keyword.addstring.identifier.minus.c5m",
          "match": "(?<=addstring.*\")\\(-\\)"
        },
        {
          "name": "keyword.addstring.identifier.plus.c5m",
          "match": "(?<=addstring.*\")\\(\\+\\)"
        },
        {
          "name": "keyword.addstring.identifier.ampersand.c5m",
          "match": "(?<=addstring.*\")\\(&\\)"
        },
        {
          "name": "keyword.addstring.identifier.familiar.c5m",
          "match": "(?<=addstring.*\")\\(!\\)"
        },
        {
          "name": "keyword.addstring.identifier.terrain.c5m",
          "match": "(?<=addstring.*\")\\(-?\\d+\\)"
        }
      ]
    },
    "addstring_operators": {
      "patterns": [
        {
          "name": "keyword.addstring.operator.and.c5m",
          "match": "(?<=addstring.*\".+)&"
        },
        {
          "name": "keyword.addstring.operator.add.c5m",
          "match": "(?<=addstring.*\".*\\d+)\\+(?=\\d+)"
        },
        {
          "name": "keyword.addstring.operator.multiply.c5m",
          "match": "(?<=addstring.*\".*\\d+)\\*(?=\\w+)"
        },
        {
          "name": "keyword.addstring.operator.commander.c5m",
          "match": "(?<=addstring.*\".*)c\\*(?!\")(?=\\D+)"
        },
        {
          "name": "keyword.addstring.operator.quantity.c5m",
          "match": "(?<=addstring.*\".*)(\\d+|\\d+d\\d+)(?=(\\*|\\+))"
        }
      ]
    }
  },
  "scopeName": "source.c5m"
