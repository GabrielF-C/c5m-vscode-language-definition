{
  "$schema": "https://raw.githubusercontent.com/martinring/tmlanguage/master/tmlanguage.json",
  "name": "Conquest of Elysium 5 Modding",
  "patterns": [
    {
      "include": "#keywords_headers"
    },
    {
      "include": "#keywords_event_triggers"
    },
    {
      "include": "#keywords_naming"
    },
    {
      "include": "#keywords_operation"
    },
    {
      "include": "#keywords_sprites"
    },
    {
      "include": "#keywords_copy"
    },
    {
      "include": "#keywords_block_start"
    },
    {
      "include": "#keywords_block_end"
    },
    {
      "include": "#keywords_general"
    },
    {
      "include": "#strings"
    },
    {
      "include": "#comments"
    },
    {
      "include": "#numbers"
    }
  ],
  "repository": {
    "strings": {
      "name": "string.quoted.double.c5m",
      "begin": "\"",
      "end": "\"",
      "patterns": [
        {
          "include": "#addstring_identifiers"
        },
        {
          "include": "#addstring_operators"
        }
      ]
    },
    "comments": {
      "patterns": [
        {
          "name": "comment.line.number-sign.c5m",
          "begin": "#",
          "beginCaptures": {
            "0": {
              "name": "comment.line.number-sign"
            }
          },
          "end": "\\n"
        }
      ]
    },
    "numbers": {
      "patterns": [
        {
          "name": "keyword.other.unit.suffix.integer.tpl",
          "match": "[0-9]+"
        }
      ]
    },
    "keywords_headers": {
      "patterns": [
        {
          "name": "keyword.headers.c5m",
          "match": "\\b(icon|description|modprio|mapfile)\\b"
        }
      ]
    },
    "keywords_event_triggers": {
      "patterns": [
        {
          "name": "keyword.event.triggers.c5m",
          "match": "\\b(aiplayer|armyowner|chance|class|hasaffunithere|hascom|hascomhere|hasitem|hasitemhere|hasunit|hasunithere|humanplayer|minanimalcorpses|mincorpses|minhumancorpses|minhumanoidcorpses|minterrains|minturnnbr|minunits|ownsloc|ownsloctarg|plane|player|season|seasonleft|seasondelay|squareactivated|squarename|squareowner|terrain|turnnbr|varequal|vargreater|varlesser|varvarequal|varvargreater|varvarlesser)\\b"
        }
      ]
    },
    "keywords_naming": {
      "patterns": [
        {
          "name": "keyword.naming.c5m",
          "match": "\\b(name|descr|setclassname|setname|cityname|comname|namesquare)\\b"
        }
      ]
    },
    "keywords_operation": {
      "patterns": [
        {
          "name": "keyword.operation.c5m",
          "match": "\\b(setvar|addvar|copyvar|addstring)\\b"
        }
      ]
    },
    "keywords_sprites": {
      "patterns": [
        {
          "name": "keyword.sprites.c5m",
          "match": "\\b(spr|spr1|spr2)\\b"
        }
      ]
    },
    "keywords_copy": {
      "patterns": [
        {
          "name": "keyword.copy.c5m",
          "match": "\\b(copystats|copyritual|copyspr)\\b"
        }
      ]
    },
    "keywords_block_start": {
      "patterns": [
        {
          "name": "keyword.block.start.c5m",
          "match": "\\b(newclass|newitem|newmonster|newritpow|newritual|newweapon|playerevent|selectclass|selectitem|selectmonster|selectritual|selectterr|selectterrgroup|selectweapon|squareevent)\\b"
        }
      ]
    },
    "keywords_block_end": {
      "patterns": [
        {
          "name": "keyword.block.end.c5m",
          "match": "\\b(endevent)\\b"
        }
      ]
    },
    "keywords_general": {
      "patterns": [
        {
          "name": "keyword.general.c5m",
